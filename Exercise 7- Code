void setup(){
 size (350, 600);
 background(255);
  frameRate(1);
}

void draw(){
 
 for (int a = 0; a < 20; a ++) {
    for (int b = 0; b < 20; b ++) {
    
   stroke(0);
   colorMode(HSB);
   fill(a * 20, b * 70, 225);
   ellipse (a *20, b * 20, 20, 20);  
   
float w = 190; 

  stroke(0);
while(w>9){
 noStroke();
 colorMode(HSB);
 fill(w++, 200, 255);
  ellipse(175,500,w,w);
  w  = w-20;
}
    }
}
}
